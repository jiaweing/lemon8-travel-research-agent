# LLM Settings
OPENAI_API_KEY=         # OpenAI API key required for content analysis
MODEL_NAME=gpt-4o-mini  # OpenAI model to use for analysis (default: gpt-4o-mini)

# Browser Settings
HEADLESS=true           # Run browser in headless mode
TIMEOUT=30              # Browser timeout in seconds

# Network Settings
#HTTP_PROXY=             # HTTP proxy URL (optional)
#HTTPS_PROXY=            # HTTPS proxy URL (optional)
#USER_AGENT=             # Custom user agent (optional)

# Output Settings
OUTPUT_DIR=output    	# Directory for scraped content

# Scraping Settings
SCROLL_DELAY=1.0        # Delay between scroll operations
